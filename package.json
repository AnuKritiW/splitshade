{
  "name": "splitshade-webgpu-playground",
  "homepage": "https://anukritiw.github.io/splitshade",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "typecheck": "vue-tsc --noEmit -p tsconfig.app.json",
    "knip": "knip --reporter compact",
    "knip:strict": "knip --strict --reporter compact",
    "tsprune": "ts-prune -p tsconfig.app.json",
    "unimported": "unimported",
    "depcheck": "depcheck --skip-missing=true"
  },
  "dependencies": {
    "@fontsource/lato": "^5.2.6",
    "@guolao/vue-monaco-editor": "^1.5.5",
    "@vicons/ionicons5": "^0.13.0",
    "monaco-editor": "^0.52.2",
    "naive-ui": "^2.42.0",
    "obj-file-parser": "^0.6.2",
    "vue": "^3.5.13",
    "wgsl_reflect": "github:brendan-duncan/wgsl_reflect"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.1.0",
    "@types/obj-file-parser": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@webgpu/types": "^0.1.63",
    "depcheck": "^1.4.7",
    "eslint": "^9.33.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-vue": "^10.4.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "knip": "^5.62.0",
    "ts-prune": "^0.10.3",
    "typescript": "~5.8.3",
    "unimported": "^1.31.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.8"
  }
}
